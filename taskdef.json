{
  "family": "cicdlab-task-def",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::266735814394:role/ECSTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::266735814394:role/cicdlab-TaskRole",
  "containerDefinitions": [
    {
      "name": "cicdlab-container",
      "image": "266735814394.dkr.ecr.eu-west-1.amazonaws.com/cicdlab-repo:latest",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cicdlab",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "cicdlab"
        }
      }
    }
  ]
}