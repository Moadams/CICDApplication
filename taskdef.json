{
  "family": "cicdlab-task-def",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::266735814394:role/ECSTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "cicdlab-container",
      "image": "<IMAGE_PLACEHOLDER>",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cicdlab",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "cicdlab"
        }
      }
    }
  ]
}
